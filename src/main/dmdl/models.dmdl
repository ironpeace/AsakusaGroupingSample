-- 入力CSVファイル形式

"OriginalData"
@directio.csv(has_header = TRUE)
original_data = {
    "keycode"
    @directio.csv.field(name = "keycode")
    keycode : INT;

    "userid"
    @directio.csv.field(name = "userid")
    userid : TEXT;

    "term"
    @directio.csv.field(name = "term")
    term : INT;

    "data"
    @directio.csv.field(name = "data")
    data : LONG;
};
