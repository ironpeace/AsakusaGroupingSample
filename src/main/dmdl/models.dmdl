joined middata1
 = original_data -> {
 	keycode -> keycode;
    userid -> userid;
	term -> term;
	data -> data0;
} % keycode
 + original_data -> {
 	keycode -> keycode;
	data -> data1;
} % keycode;

middata2 = {
    "keycode"
    keycode : INT;

    "alert"
    alert : TEXT;
};

joined pre_result
 = middata1 -> {
 	keycode -> keycode;
    userid -> userid;
	term -> term;
	data0 -> data0;
	data1 -> data1;
} % keycode 
 + middata2 -> {
 	keycode -> keycode;
    alert -> alert;
} % keycode; 

"Result"
@directio.csv(has_header = TRUE)
result = {

    "keycode"
    @directio.csv.field(name = "keycode")
    keycode : INT;

    "userid"
    @directio.csv.field(name = "userid")
    userid : TEXT;

    "term"
    @directio.csv.field(name = "term")
    term : INT;

    "data0"
    @directio.csv.field(name = "data0")
    data0 : LONG;

    "data1"
    @directio.csv.field(name = "data1")
    data1 : LONG;

    "alert"
    @directio.csv.field(name = "alert")
    alert : TEXT;
};

